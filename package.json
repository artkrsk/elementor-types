{
  "name": "@arts/elementor-types",
  "version": "1.0.0",
  "description": "TypeScript type definitions for Elementor WordPress plugin",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prepack": "pnpm run clean && pnpm run build",
    "type-check": "tsc --noEmit",
    "lint": "tsc --noEmit --strict"
  },
  "keywords": [
    "elementor",
    "wordpress",
    "typescript",
    "types",
    "definitions"
  ],
  "author": "Artem Semkin",
  "license": "MIT",
  "devDependencies": {
    "@types/backbone": "^1.4.23",
    "@types/jquery": "^3.5.29",
    "@types/node": "^24.4.0",
    "@types/select2": "^4.0.63",
    "backbone": "^1.6.1",
    "backbone.marionette": "^4.1.3",
    "backbone.radio": "^2.0.0",
    "swiper": "^12.0.1",
    "typescript": "^5.2.0",
    "underscore": "^1.13.7"
  },
  "peerDependencies": {
    "@types/jquery": "^3.5.0",
    "swiper": "^11.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
